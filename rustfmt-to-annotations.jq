{name: $github_job_name, head_sha: $github_pr_head_sha, output: { title: "Rustfmt Results", summary: "This action has included some annotations about format changes you need to make.", annotations: [.[] | .mismatches[] as $mis | {path: (.name | sub($pwd; "")), start_line: $mis.original_begin_line, end_line: $mis.original_end_line, annotation_level: "failure", message: ("```diff\n-" + ($mis.original | split("\n") | join("\n-")) + "\n+" + ($mis.expected | split("\n") | join("\n+")) + "\n```")}] }}